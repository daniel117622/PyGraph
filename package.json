{
  "name": "python-calltree-viewer",
  "displayName": "Python Calltree Viewer",
  "description": "Generate and view static Python call trees for selected functions or methods directly inside Visual Studio Code.",
  "publisher": "daniel117622",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel117622/PyGraph.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:pythonCalltree.showCallTree"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pythonCallTreeSidebar",
          "title": "Call Tree",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "pythonCallTreeSidebar": [
        {
          "id": "pythonCallTree",
          "name": "Python Call Tree"
        }
      ]
    },
    "commands": [
      {
        "command": "pythonCalltree.showCallTree",
        "title": "Show Python Call Tree"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "pythonCalltree.showCallTree",
          "when": "resourceLangId == python",
          "group": "navigation@2"
        }
      ]
    },
    "keybindings": [
      {
        "command": "pythonCalltree.showCallTree",
        "key": "ctrl+shift+r",
        "when": "resourceLangId == python"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "@types/vscode": "^1.60.0",
    "prettier": "^2.8.8",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "filbert": "^0.1.20"
  }
}